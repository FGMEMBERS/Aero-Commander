<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <path>pedals.ac</path>

  <animation>
    <object-name>base</object-name>
    <object-name>LHpedal</object-name>
    <object-name>RHpedal</object-name>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>LHpedal</object-name>
    <property>surface-positions/rudder-pos-norm</property>
    <interpolation>
      <entry><ind> -1 </ind><dep> -25 </dep></entry>
      <entry><ind>  0 </ind><dep>   0 </dep></entry>
      <entry><ind>  1 </ind><dep>  15 </dep></entry>
    </interpolation>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>RHpedal</object-name>
    <property>surface-positions/rudder-pos-norm</property>
    <interpolation>
      <entry><ind> -1 </ind><dep> -25 </dep></entry>
      <entry><ind>  0 </ind><dep>   0 </dep></entry>
      <entry><ind>  1 </ind><dep>  15 </dep></entry>
    </interpolation>
    <axis>
      <x>  0 </x>
      <y> -1 </y>
      <z>  0 </z>
    </axis>
  </animation>

</PropertyList>
