<PropertyList>

  <path>overhead.ac</path>

  <animation>
    <!-- Objets opaques -->
    <object-name>overhead</object-name>
	<object-name>batt</object-name>
    
    <!-- Objets transparents -->
  </animation>

  <!-- Pedestal -->
  <model>
    <path>Aircraft/Aero-Commander/Models/Interior/Panel/Overhead/Instruments/Magnetos/magnetoL.xml</path>
    <offsets>
      <x-m>  0.005412 </x-m>
      <y-m> -0.038260 </y-m>
      <z-m>  0.001585 </z-m>
      <heading-deg> 8 </heading-deg>
    </offsets>
  </model>

  <model>
    <path>Aircraft/Aero-Commander/Models/Interior/Panel/Overhead/Instruments/Magnetos/magnetoR.xml</path>
    <offsets>
      <x-m> 0.005412 </x-m>
      <y-m> 0.038260 </y-m>
      <z-m> 0.001585 </z-m>
      <heading-deg> -8 </heading-deg>
    </offsets>
  </model>

  <!-- Starter -->
