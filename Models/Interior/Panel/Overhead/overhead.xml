    <path>Aircraft/Aero-Commander/Models/Interior/Panel/Overhead/Instruments/Starter/startL.xml</path>
    <offsets>
      <x-m>  0.009549 </x-m>
      <y-m> -0.091090 </y-m>
      <z-m>  0.025080 </z-m>
      <heading-deg> 8 </heading-deg>
    </offsets>
  </model>

  <model>
    <path>Aircraft/Aero-Commander/Models/Interior/Panel/Overhead/Instruments/Starter/startR.xml</path>
