      <x-m> 0.009549 </x-m>
      <y-m> 0.091090 </y-m>
      <z-m> 0.025080 </z-m>
      <heading-deg> -8 </heading-deg>
    </offsets>
  </model>
  
      <animation>
        <type>rotate</type>
        <object-name>batt</object-name>
        <property>controls/electric/battery-switch</property>
    <interpolation>
      <entry><ind> 0 </ind><dep> -3 </dep></entry>
      <entry><ind> 1 </ind><dep> 5 </dep></entry>
    </interpolation>
        <center>
            <x-m>0.04767</x-m>
            <y-m>-0.32821</y-m>
            <z-m>0.00917</z-m>
        </center>
        <axis>
            <x>0.0</x>
            <y>1</y>
            <z>0</z>
        </axis>
    </animation>
	
	    <animation>
        <type>pick</type>
        <object-name>batt</object-name>
        <action>
            <button>0</button>
            <binding>
                <command>property-toggle</command>
                <property>controls/electric/battery-switch</property>
            </binding>
        </action>
    </animation>
	
