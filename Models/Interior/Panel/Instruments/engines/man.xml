<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <path>man.ac</path>

  <animation>
    <object-name>fond</object-name>
    <object-name>face</object-name>
    <object-name>needleL</object-name>
    <object-name>needleR</object-name>
    
    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>noshadow</type>
    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>shader</type>
    <shader>chrome</shader>
    <texture>Aircraft/Generic/Effects/glass_shader.png</texture>
    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>material</type>
    <object-name>face</object-name>
    <object-name>needleL</object-name>
    <object-name>needleR</object-name>
    <condition>
      <property>/controls/lighting/instrument-lights</property>
    </condition>
    <emission>
      <red>   0.80 </red>
      <green> 0.50 </green>
      <blue>  0.50 </blue>
    </emission>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>needleL</object-name>
    <property>engines/engine[0]/mp-osi</property>
    <interpolation>
      <entry><ind>  0 </ind><dep>   0 </dep></entry>
      <entry><ind> 1 </ind><dep>  17 </dep></entry>
      <entry><ind> 6 </ind><dep>  68 </dep></entry>
      <entry><ind> 20 </ind><dep> 118 </dep></entry>
      <entry><ind> 40 </ind><dep> 169 </dep></entry>
      <entry><ind> 50 </ind><dep> 218 </dep></entry>
      <entry><ind> 60 </ind><dep> 268 </dep></entry>
      <entry><ind> 70 </ind><dep> 317 </dep></entry>
      <entry><ind> 75 </ind><dep> 342 </dep></entry>
    </interpolation>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>
  
  <animation>
    <type>rotate</type>
    <object-name>needleR</object-name>
    <property>engines/engine[1]/mp-osi</property>
    <interpolation>
      <entry><ind>  0 </ind><dep>   0 </dep></entry>
      <entry><ind> 1 </ind><dep>  17 </dep></entry>
      <entry><ind> 6 </ind><dep>  68 </dep></entry>
      <entry><ind> 20 </ind><dep> 118 </dep></entry>
      <entry><ind> 40 </ind><dep> 169 </dep></entry>
      <entry><ind> 50 </ind><dep> 218 </dep></entry>
      <entry><ind> 60 </ind><dep> 268 </dep></entry>
      <entry><ind> 70 </ind><dep> 317 </dep></entry>
      <entry><ind> 75 </ind><dep> 342 </dep></entry>
    </interpolation>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>
  
</PropertyList>
